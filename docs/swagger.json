  "definitions": {
    "ClientRegister": {
      "required": [
        "name",
        "chipset",
        "mac",
        "serial",
        "processor",
        "channel",
        "location"
      ],
      "type": "object",
      "properties": {
        "clientTime": {
          "type": "number",
          "example": 1000000000.1111
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string",
            "example": "example-tag"
          }
        },
        "name": {
          "type": "string",
          "example": "Raspberry PI"
        },
        "chipset": {
          "type": "string",
          "example": "AMD 790FX"
        },
        "mac": {
          "type": "string",
          "example": "FF:FF:FF:FF:FF:FF"
        },
        "serial": {
          "type": "string",
          "example": "C210"
        },
        "processor": {
          "type": "string",
          "example": "Intel I3"
        },
        "channel": {
          "type": "string",
          "example": "Ethernet"
        },
        "location": {
          "type": "string",
          "example": "-15.7757876:-48.077829"
        }
      }
    },
    "ClientRegisterResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "example": 200
        },
        "message": {
          "type": "string",
          "example": "Success"
        },
        "token": {
          "type": "string",
          "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbiI6Ijk3NDc5NTgzZjMwZDRiNTViOTRkZDkyZTAxZDk0ZWM1ZjNhMDliNzAifQ.WUHHq-34xu030Ieqgkh2owL94TCyI4nMpXIICLbqEeA"
        }
      }
    },
    "ClientResponse": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "example": "Raspberry PI"
        },
        "chipset": {
          "type": "string",
          "example": "AMD 790FX"
        },
        "mac": {
          "type": "string",
          "example": "FF:FF:FF:FF:FF:FF"
        },
        "serial": {
          "type": "string",
          "example": "C210"
        },
        "processor": {
          "type": "string",
          "example": "Intel I3"
        },
        "channel": {
          "type": "string",
          "example": "Ethernet"
        },
        "location": {
          "type": "string",
          "example": "-15.7757876:-48.077829"
        },
        "clientTime": {
          "type": "number",
          "example": 1000000000.1
        }
      }
    },
    "ClientRevalidate": {
      "required": [
        "location"
      ],
      "type": "object",
      "properties": {
        "location": {
          "type": "string",
          "example": "-15.7757876:-48.077829"
        }
      }
    },
    "ClientRevalidateResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "example": 200
        },
        "message": {
          "type": "string",
          "example": "Success"
        },
        "token": {
          "type": "string",
          "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbiI6Ijk3NDc5NTgzZjMwZDRiNTViOTRkZDkyZTAxZDk0ZWM1ZjNhMDliNzAifQ.WUHHq-34xu030Ieqgkh2owL94TCyI4nMpXIICLbqEeA"
